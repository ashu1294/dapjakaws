
server {
    listen 80;
    server_name ec2-13-232-71-40.ap-south-1.compute.amazonaws.com;

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/dapjak-blog/app.sock;
    }
    location /static/ {
        autoindex on;
        alias /home/ubuntu/dapjak-blog/blog/static/;
    }
}
